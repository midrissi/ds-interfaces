{
  "name": "ds-interfaces",
  "version": "0.2.3",
  "description": "Outputs interfaces from a wakanda catalog",
  "main": "./src/index.js",
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.js\" \"__tests__/**/*.js\"",
    "pretest": "npm run lint",
    "test": "nyc --reporter=html --reporter=text mocha --exit --reporter spec ./__tests__/**/*.test.js",
    "prepublishOnly": "npm test",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "release:major": "release-it major",
    "release:minor": "release-it minor",
    "release:patch": "release-it patch",
    "prepare": "husky install"
  },
  "repository": {
    "url": "https://github.com/midrissi/ds-interfaces.git",
    "type": "git"
  },
  "keywords": [
    "function",
    "location",
    "nodejs",
    "node",
    "wakanda",
    "promise"
  ],
  "author": {
    "name": "4D SAS",
    "email": "contact@4d.com",
    "url": "https://www.4d.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "eslint": "^8.1.0",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.14",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "release-it": "^14.11.6"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "files": [
    "src/**/*.js",
    "templates/*",
    "bin/*"
  ],
  "dependencies": {
    "nunjucks": "^3.2.3",
    "request": "^2.88.2"
  },
  "bin": {
    "ds-interfaces": "./bin/ds-interfaces"
  }
}
