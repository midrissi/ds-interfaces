{
  "name": "ds-interfaces",
  "version": "0.2.1",
  "description": "Outputs interfaces from a wakanda catalog",
  "main": "./src/index.js",
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.js\" \"__tests__/**/*.js\"",
    "pretest": "npm run lint",
    "test": "nyc --reporter=html --reporter=text mocha --exit --reporter spec ./__tests__/**/*.test.js",
    "prepublishOnly": "npm test",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "release:major": "release-it major",
    "release:minor": "release-it minor",
    "release:patch": "release-it patch"
  },
  "repository": {
    "url": "https://github.com/midrissi/ds-interfaces.git",
    "type": "git"
  },
  "keywords": [
    "function",
    "location",
    "nodejs",
    "node",
    "wakanda",
    "promise"
  ],
  "author": {
    "name": "Mohamed IDRISSI",
    "email": "med.idrissi@outlook.com",
    "url": "https://github.com/midrissi"
  },
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.9",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "release-it": "^12.4.3"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.1"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "files": [
    "src/**/*.js",
    "templates/*",
    "bin/*"
  ],
  "dependencies": {
    "nunjucks": "^3.2.0",
    "request": "^2.88.0"
  },
  "bin": {
    "ds-interfaces": "./bin/ds-interfaces"
  }
}
